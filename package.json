{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "docs": "typedoc --out docs ./src --excludeInternal; touch ./docs/.nojekyll"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "soundcloud-downloader",
  "author": "Zack Radisic",
  "module": "dist/soundcloud-downloader.esm.js",
  "size-limit": [
    {
      "path": "dist/soundcloud-downloader.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/soundcloud-downloader.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.2",
    "axios-mock-adapter": "^1.19.0",
    "file-type": "^16.4.0",
    "husky": "^6.0.0",
    "music-metadata": "^7.8.5",
    "size-limit": "^4.10.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "dotenv": "^9.0.2",
    "m3u8stream": "^0.8.3",
    "soundcloud-key-fetch": "^1.0.11"
  }
}
